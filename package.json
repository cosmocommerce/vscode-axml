{
    "name": "vscode-axml",
    "displayName": "vscode axml",
    "description": "Ant axml support/snippets for vscode",
    "version": "0.1.7",
    "icon": "images/weapp.png",
    "publisher": "CosmoCommerce",
    "author": {
        "email": "gongyuan.mao@jorigin.cn",
        "name": "CosmoCommerce",
        "url": "http://www.cosmocommerce.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cosmocommerce/vscode-axml"
    },
    "bugs": {
        "url": "https://github.com/cosmocommerce/vscode-axml/issues",
        "email": "gongyuan.mao@jorigin.cn"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Snippets",
        "Other"
    ],
    "keywords": [
        "antapp",
        "alibaba",
        "ant",
        "支付宝",
        "axml"
    ],
    "contributes": {
        "languages": [
            {
                "id": "axml",
                "aliases": [
                    "axml",
                    "axml"
                ],
                "extensions": [
                    ".axml"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "axml",
                "scopeName": "text.html.axml",
                "path": "./syntaxes/axml.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "axml",
                "path": "./snippets/axml.json"
            }
        ]
    }
}